<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\MntProveedor" table="mnt_proveedor">
    <indexes>
      <index name="IDX_71E5C3606325E299" columns="id_departamento"/>
      <index name="IDX_71E5C3607EAD49C7" columns="id_municipio"/>
      <index name="IDX_71E5C360F57D32FD" columns="id_pais"/>
      <index name="IDX_71E5C3602D3EB78E" columns="id_tamanioproveedor"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="mnt_proveedor_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="60" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registro" type="string" column="registro" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dui" type="string" column="dui" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nit" type="string" column="nit" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="giro" type="string" column="giro" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="exento" type="boolean" column="exento" nullable="true"/>
    <field name="extranjero" type="boolean" column="extranjero" nullable="true"/>
    <field name="userAdd" type="integer" column="user_add" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="false"/>
    <field name="userMod" type="integer" column="user_mod" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateMod" type="datetime" column="date_mod" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <many-to-one field="idTamanioproveedor" target-entity="CtlTamanio" fetch="LAZY">
      <join-columns>
        <join-column name="id_tamanioproveedor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPais" target-entity="CtlPais" fetch="LAZY">
      <join-columns>
        <join-column name="id_pais" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMunicipio" target-entity="CtlMunicipio" fetch="LAZY">
      <join-columns>
        <join-column name="id_municipio" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDepartamento" target-entity="CtlDepartamento" fetch="LAZY">
      <join-columns>
        <join-column name="id_departamento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
