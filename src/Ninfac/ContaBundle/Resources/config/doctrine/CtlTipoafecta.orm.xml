<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ninfac\ContaBundle\Entity\CtlTipoafecta" table="ctl_tipoafecta">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="ctl_tipoafecta_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="40" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">nombre como se expresa la cuenta</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
